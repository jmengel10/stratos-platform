{
  "name": "stratos-platform",
  "version": "1.0.0",
  "private": true,
  "description": "StratOS - AI-powered strategy consulting and venture capital platform",
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend & npm run start --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run start --workspace=backend",
    "build": "npm run build --workspaces",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "clean": "npm run clean --workspaces && rm -rf node_modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/stratos-platform.git"
  },
  "keywords": [
    "ai",
    "strategy",
    "consulting",
    "venture-capital",
    "azure",
    "openai"
  ],
  "author": "StratOS Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

